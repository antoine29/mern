{
  "name": "mern",
  "version": "1.0.0",
  "description": "mern by antoine",
  "main": "index.js",
  "scripts": {
    "compileBabel": "babel src/ --presets react,es2015 --out-dir static",
    "compileJSX": "jsx --run",
    "watchBabel": "babel src/ --presets react,es2015 --out-dir static --watch",
    "compileWebPack": "webpack",
    "watchWebPack": "webpack --watch",
    "watchWebPackDevServer": "webpack-dev-server --hot --inline",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon -w dist dist/server.js",
    "start2": "nodemon -w server server/server.js",
    "startHook": "nodemon -w server server/start_hook.js",
    "compileServer": "babel server --presets node8 --out-dir dist --source-maps",
    "watchServer": "babel server --presets node8 --out-dir dist --source-maps --watch",
    "dev-all": "npm run watchWebPackDevServer & npm run watchServer & npm run start",
    "dev-all-hook": "npm run watchWebPackDevServer & npm run watchServer & npm run startHook"
  },
  "author": "antoine",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "eslint": "^4.19.0",
