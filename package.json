    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "express": "^4.16.2",
    "jsx": "^0.9.89",
    "mongodb": "^3.0.2",
    "npm-install-missing": "^0.1.4",
    "source-map-support": "^0.5.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-list": "0.0.1",
