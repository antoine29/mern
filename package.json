    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint-config-airbnb": "^16.1.0",
    "hawk": "^3.1.3",
    "nodemon": "^1.17.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^2.0.11",
    "webpack-dev-middleware": "^3.0.1",
    "webpack-dev-server": "^3.1.1",
